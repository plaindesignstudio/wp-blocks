{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/card-dynamic-block",
  "version": "0.1.0",
  "title": "Card Dynamic Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false,
    "align": [
      "wide"
    ]
  },
  "attributes": {
    "layout_variant": {
      "enum": [
        "text-left",
        "text-right"
      ],
      "default": "text-right"
    },
    "image_id": {
      "type": "number"
    },
    "image_url": {
      "type": "string"
    },
    "heading": {
      "type": "string"
    },
    "body": {
      "type": "string"
    }
  },
  "textdomain": "card-dynamic-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}